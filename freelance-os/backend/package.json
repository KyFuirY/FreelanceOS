{
  "name": "@freelance-os/backend",
  "version": "1.0.0",
  "description": "API REST Backend pour FreelanceOS - Node.js + Fastify + TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:pm2": "pm2 start ecosystem.config.json",
    "stop:pm2": "pm2 stop freelance-os-backend",
    "restart:pm2": "pm2 restart freelance-os-backend",
    "logs:pm2": "pm2 logs freelance-os-backend",
    "status:pm2": "pm2 status",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest --config vitest.config.e2e.ts",
    "lint": "biome check src/",
    "lint:fix": "biome check --apply-unsafe src/",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "api",
    "fastify",
    "typescript",
    "prisma",
    "freelance"
  ],
  "author": "FreelanceOS Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "^5.8.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.5",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.8",
    "puppeteer": "^21.9.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/nodemailer": "^6.4.14",
    "@vitest/coverage-v8": "^1.2.1",
    "prisma": "^5.8.1",
    "tsc-alias": "^1.8.8",
    "vitest": "^1.2.1"
  }
}